{
    "env": {
        "browser": true,
        "commonjs": true,
        "jest": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    //"extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "global-require": "off",
        "arrow-spacing": 1,
        "camelcase": 1,
        "import/prefer-default-export": "off",
        "padding-line-between-statements": "error",
        "operator-linebreak": "off"

    }
}
